{
  "name": "UpWork Job Filter",
  "version": "1.0",
  "description": "Filter jobs by matched words",
  "manifest_version": 2,
  "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
  },
  "browser_action": {
      "default_icon": "icon16.png",
      "default_popup": "./popup-ui/index.html"
  },
  "background": {
      "scripts": ["background.js"],
      "persistent": false
  }
}